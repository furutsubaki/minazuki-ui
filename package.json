{
  "name": "minazuki-ui",
  "version": "1.4.7",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@9.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/furutsubaki/minazuki-ui"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": ">=20.5.*"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm create-component-d && run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "create-component-d": "node ./src/generate-component-index.js && prettier --write ./src/components/index.ts",
    "eslint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "eslint:fix": "pnpm eslint --fix",
    "format": "prettier --write src/",
    "lint:es": "pnpm eslint",
    "lint:es:fix": "pnpm eslint:fix && pnpm format",
    "lint:style": "stylelint \"**/*.css\" \"**/*.scss\" \"**/*.vue\"",
    "lint:style:fix": "pnpm run lint:style --fix",
    "lint": "pnpm run lint:es && pnpm run lint:style",
    "lint:fix": "pnpm run lint:es:fix && pnpm run lint:style:fix",
    "sb": "pnpm storybook dev --port 6006 --no-open",
    "sb:test": "test-storybook --url http://localhost:6006",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@acab/reset.css": "^0.11",
    "@vee-validate/rules": "^4",
    "@vee-validate/zod": "^4",
    "i18next": "^23",
    "pinia": "^2",
    "vee-validate": "^4",
    "vue": "^3",
    "zod": "^3",
    "zod-i18n-map": "^2"
  },
  "dependencies": {
    "@acab/reset.css": "^0.11.0",
    "@unhead/vue": "^1.11.19",
    "@vee-validate/rules": "^4.13.2",
    "@vee-validate/zod": "^4.14.7",
    "@vuepic/vue-datepicker": "^11.0.2",
    "dayjs": "^1.11.11",
    "i18next": "^23.12.2",
    "lucide-vue-next": "^0.356.0",
    "pinia": "^2.2.2",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.27",
    "vue-router": "^4.5.1",
    "zod": "^3.25.20",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-backgrounds": "^8.6.14",
    "@storybook/addon-controls": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-measure": "^8.6.14",
    "@storybook/addon-outline": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/preview-api": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "@tsconfig/node18": "^18.2.4",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.14.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "lodash.merge": "^4.6.2",
    "npm-run-all2": "^6.2.2",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.6.14",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "type-fest": "^4.38.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.17"
  }
}
